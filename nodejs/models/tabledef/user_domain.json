{
    "name": "user_domain",
    "version": "0.0.1",
    "description": "ユーザーの所属するドメインを管理するテーブル",
    "scope" : "protected",
    "fields": [
        {
            "name": "id",
            "type": "INTEGER",
            "primaryKey": true,
            "autoIncrement": true
        },
        {
            "name": "domain_name",
            "type": "STRING",
            "length": 255,
            "notNull": true
        },
        {
            "name": "domain_description",
            "type": "TEXT"
        },
        {
            "name": "created_at",
            "type": "DATETIME",
            "default": "CURRENT_TIMESTAMP"
        },
        {
            "name": "updated_at",
            "type": "DATETIME",
            "default": "CURRENT_TIMESTAMP"
        },
        {
            "name": "deleted_at",
            "type": "DATETIME",
            "nullable": true
        }
    ]
}