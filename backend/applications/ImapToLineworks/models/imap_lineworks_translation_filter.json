{
    "name": "imap_lineworks_translation_filter",
    "version": "0.0.1",
    "description": "imap_lineworks_translation_filter. IMAP から LINE WORKS への通知を行う際のフィルタリング条件を管理するテーブル",
    "scope": "protected",
    "user_scope": "user_readwrite",
    "fields": [
        {
            "name": "filter_id",
            "type": "INTEGER",
            "primaryKey": true,
            "autoIncrement": true
        },
        {
            "name": "user_domain_id",
            "type": "INTEGER",
            "notNull": true,
            "foreignKey": {
                "table": "user_domain",
                "field": "user_domain_id"
            }
        },
        {
            "name": "user_id",
            "type": "INTEGER",
            "notNull": true,
            "foreignKey": {
                "table": "user",
                "field": "user_id"
            }
        },
        {
            "name": "imap_setting_id",
            "type": "INTEGER",
            "notNull": true,
            "foreignKey": {
                "table": "imap_setting",
                "field": "imap_setting_id"
            }
        },
        {
            "name": "lineworks_setting_id",
            "type": "INTEGER",
            "notNull": true,
            "foreignKey": {
                "table": "lineworks_setting",
                "field": "lineworks_setting_id"
            }
        },
        {
            "name": "filter_name",
            "type": "STRING",
            "length": 255,
            "notNull": true
        },
        {
            "name": "search_filter",
            "type": "STRING",
            "length": 255,
            "notNull": false,
            "default": "UNSEEN"
        },
        {
            "name": "filter_type",
            "type": "STRING",
            "length": 255,
            "notNull": true,
            "description": "フィルタリング条件の種類 {from: , subject: , body: }"
        },
        {
            "name": "filter_value",
            "type": "STRING",
            "length": 255,
            "notNull": true,
            "description": "フィルタリング条件の値。部分一致で評価される"
        },
        {
            "name": "lineworks_user_account",
            "type": "STRING",
            "length": 255,
            "notNull": true,
            "description": "LINE WORKS のユーザアカウント。"
        },
        {
            "name": "created_at",
            "type": "DATETIME",
            "default": "CURRENT_TIMESTAMP"
        },
        {
            "name": "updated_at",
            "type": "DATETIME",
            "default": "CURRENT_TIMESTAMP"
        },
        {
            "name": "deleted_at",
            "type": "DATETIME",
            "nullable": true
        }
    ]
}
