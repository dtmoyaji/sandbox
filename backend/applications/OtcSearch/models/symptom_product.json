{
    "name": "symptom_product",
    "version": "0.0.1",
    "description": "症状と商品の関連付けを管理するテーブル",
    "scope": "protected",
    "user_scope": "user_admin_readwrite",
    "fields": [
        {
            "name": "symptom_product_id",
            "type": "INTEGER",
            "primaryKey": true,
            "autoIncrement": true
        },
        {
            "name": "symptom_name",
            "type": "STRING",
            "length": 255,
            "notNull": true
        },
        {
            "name": "product_id",
            "type": "INTEGER",
            "notNull": true
        },
        {
            "name": "relevance_score",
            "type": "FLOAT",
            "default": 1.0,
            "notNull": true
        },
        {
            "name": "recommendation_notes",
            "type": "TEXT"
        },
        {
            "name": "created_at",
            "type": "DATETIME",
            "default": "CURRENT_TIMESTAMP"
        },
        {
            "name": "updated_at",
            "type": "DATETIME",
            "default": "CURRENT_TIMESTAMP"
        },
        {
            "name": "deleted_at",
            "type": "DATETIME"
        }
    ]
}